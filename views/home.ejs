<% include ./partials/header %>

<section class="page-section clearfix">
    
    <div class="container">
        <div class="row justify-content-md-center"  id="ilanlar">

        
        <%foundPosts.forEach( (d)=>{ %>

            <div class="card" style="width: 18rem;">
                    <img class="card-img-top" src="/public/images/<%= d.postImage %>" alt="Card image cap">
                    <div class="card-body" id="ilan">
                    <h3 class="card-title"><%= d.postTitle %></h5>
                    <h5 class="card-text"><%= d.postType %></h1>
                    <h5 class="card-text">test</h1>
                    <button onclick="myFunction(this.id)" id="<%= d.postOwnerPhone %>"class="btn btn-primary">Telefon NumarasÄ±</button>
                    
                    <p class="card-text"><%= d.postNote %></p>
                    </div>
                </div>
        <%  }); %>
        </div>

      </div>

</section>

<script>
 
    function myFunction(clicked_id) {
       confirm(clicked_id);
    }
</script>



